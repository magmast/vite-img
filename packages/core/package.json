{
  "name": "@vite-img/core",
  "version": "1.0.0-alpha.1",
  "license": "MIT",
  "type": "module",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write --ignore-path ../../.gitignore .",
    "typecheck": "tsc",
    "publish": "bun publish"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "sharp": ">=0.33.0"
  }
}
