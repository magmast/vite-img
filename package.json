{
  "name": "@vite-img/root",
  "private": true,
  "workspaces": [
    "./packages/*",
    "./examples/*"
  ],
  "devDependencies": {
    "turbo": "^2.3.3"
  },
  "packageManager": "bun@1.1.34",
  "scripts": {
    "dev": "turbo dev --filter=./packages/*",
    "build": "turbo build --filter=./packages/*",
    "format": "turbo format",
    "typecheck": "turbo typecheck",
    "publish": "turbo publish --filter=./packages/*"
  }
}
