{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {},
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "publish": {
      "cache": false,
      "dependsOn": ["^publish", "build"]
    }
  }
}
